{
  "version": 2,
  "functions": {
    "api/server.js": {
      "memory": 1024,
      "includeFiles": "api/db.json"
    }
  },
  "builds": [
    {
      "src": "api/server.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/server.js"
    }
  ]
}




  
  
  